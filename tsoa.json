{
    "entryFile": "index.ts",
    "noImplicitAdditionalProperties": "throw-on-extras",
    "spec": {
        "outputDirectory": "public",
        "specVersion": 3,
        "securityDefinitions": {
            "jwt": {
                "type": "apiKey",
                "in": "cookie",
                "name": "token"
            },
            "headerjwt": {
                "type": "apiKey",
                "in": "header",
                "name": "x-access-token"
            }
        }
    },
    "routes": {
        "routesDir": "routes",
        "authenticationModule": "./Middleware/jwtauth.ts"
    }
}